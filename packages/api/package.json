{
  "name": "@mastertrack/api",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./db/schemas": "./src/db/schemas.ts"
  },
  "scripts": {
    "dev": "bun --env-file=../../.env run --hot src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun run dist/index.js",
    "db:push": "bun --env-file=../../.env x drizzle-kit push --force",
    "db:generate": "bun --env-file=../../.env x drizzle-kit generate",
    "db:migrate": "bun --env-file=../../.env x drizzle-kit migrate",
    "db:seed": "bun --env-file=../../.env run src/db/seed.ts",
    "db:studio": "bun --env-file=../../.env x drizzle-kit studio"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.4.2",
    "@mastertrack/shared": "workspace:*",
    "@supabase/supabase-js": "^2.47.12",
    "drizzle-orm": "^0.38.3",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.6.14",
    "hono-rate-limiter": "^0.5.0",
    "postgres": "^3.4.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "bun-types": "latest",
    "drizzle-kit": "^0.30.1"
  }
}
